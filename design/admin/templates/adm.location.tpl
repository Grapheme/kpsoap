{strip}{if $SITE_SECTION_NAME == ""}            	<a href="{$SERVER_URL_NAME}admin/">Панель управления</a>{elseif !empty($SITE_SECTION_NAME) && empty($SITE_SECTION_PAGE)}	<a href="{$SERVER_URL_NAME}admin/">Панель управления</a>&nbsp;&raquo;&nbsp;	{section name=location loop=$menu_list}                		{if $menu_list[location].alias == $SITE_SECTION_NAME}			<a href="{$SERVER_URL_NAME}admin/{$menu_list[location].alias}/">{$menu_list[location].name}</a>		{/if}	{/section}{elseif !empty($SITE_SECTION_NAME) && !empty($SITE_SECTION_PAGE)}	<a href="{$SERVER_URL_NAME}admin/">Панель управления</a>&nbsp;&raquo;&nbsp;	{section name=location loop=$menu_list}                		{if $menu_list[location].alias == $SITE_SECTION_NAME}			{if is_array($menu_list[location].subpage)}				<a href="{$SERVER_URL_NAME}admin/{$menu_list[location].alias}/">{$menu_list[location].name}</a>&nbsp;&raquo;&nbsp;				{section name=subloc loop=$menu_list[location].subpage}					{if $menu_list[location].subpage[subloc].alias == $SITE_SECTION_PAGE}						<a href="{$SERVER_URL_NAME}admin/{$menu_list[location].alias}/{$menu_list[location].subpage[subloc].alias}">{$menu_list[location].subpage[subloc].menutitle}</a>					{/if}				{/section}			{else}				{$menu_list[location].name}1			{/if}		{/if}	{/section}{/if}{/strip}