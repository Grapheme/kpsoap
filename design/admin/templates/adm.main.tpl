<table class="panels">	<tr>		<td class="panel">			<div class="title" style="text-align: center"><h2>Панель управления <b>{$SERVER_URL_NAME|regex_replace:"/^http:\/\/([^\/]*)\/$/i":"$1"}</b></h2></div>			<div class="main-page">				{section name=menu loop=$menu_list}					{strip}<a href="{$SERVER_URL_NAME}admin/{if !empty($menu_list[menu].alias)}{$menu_list[menu].alias}/{/if}">						<img src="/module/admin/ico.{$menu_list[menu].alias}.png" width="160" height="160" /><br />						{$menu_list[menu].name}					</a>{/strip}				{/section}				</tr>			</div>		</td>	</tr></table>